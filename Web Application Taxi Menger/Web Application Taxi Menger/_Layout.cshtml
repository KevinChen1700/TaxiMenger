@{
	string rol = Session["Rol"].ToString();

}
<!DOCTYPE html>
<html>
<head>
	<!--Import Google Icon Font-->
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
	<!--Import materialize.css-->
	<link type="text/css" rel="stylesheet" href="css/materialize.min.css" media="screen,projection" />
	<!--Let browser know website is optimized for mobile-->
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body>
	<!--Import jQuery before materialize.js-->
	<script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
	<script type="text/javascript" src="js/materialize.min.js"></script>
	<script src="/js/Pagination-Mirjam/materialize-pagination.js"></script>
	<script type="text/javascript" src="js/init.js"></script>
	<script type="text/javascript" src="~/js/ExportScript.js"></script>
	<div class="row">
		<div class="col s0 m0 l2">
			@if (rol == "Chauffeur")
			{
				@RenderPage("_NavbarChauffeur.cshtml")
			}
			else
			{
				@RenderPage("_Navbar.cshtml")
			}
		</div>
		<div class="col s12 m12 l10">
			<div class="container">
				@RenderBody()
			</div>
		</div>
	</div>
</body>

</html>